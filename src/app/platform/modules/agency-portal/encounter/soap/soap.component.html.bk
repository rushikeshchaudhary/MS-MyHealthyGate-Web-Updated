                    <button *ngIf="encounterId > 0" mat-raised-button color="primary" (click)="onSubmit()" [disabled]="submitted || (encounterId > 0 && !isAllSigned)"
                      class="text-uppercase ml-3">
                        {{submitted ? 'Completing...' : 'Complete'}}</button>
                  </div>

